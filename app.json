{
  "expo": {
    "name": "hairenjun's WebDEMO",
    "slug": "WebDEMO",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hairenjun.WebDEMO"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hairenjun.WebDEMO"
    },
    "web": {
      "favicon": "./assets/favicon.jpeg"
    }
  }
}
